<div class="tab">
            <nav>
            <ul>
                <li><button class="tablinks" onclick="inventoryTab(event, 'bought-items')" >Bought Items</button></li>
                <li> <button id="defaultOpen" class="tablinks" onclick="inventoryTab(event, 'suggested-items') ">Suggested Items</button></li>
            </ul>
            </nav>

        </div>


<section id="bought-items" class="items-container tabcontent">
    <h2 class="main-title">Bought Items</h2>
    <div class="bought-container">

        {{#each boughtItems}}

        <div class="items-overview">
            <h3 class="newItemH3">{{this.title}}</h3>
            <div class="icons">
                <img class="bought-icon" src='{{this.img_link}}'>
                <button class="new-item-button" id="{{this.id}}" onclick="window.location.href='/inventory/bought-item/{{this.id}}'">
                    <img class="help-icon" src="../../public/imgs/help.svg" alt="">
                </button>
            </div>
        </div>
        </div>

        {{/each}}


</section>


<section id="suggested-items" class="items-container tabcontent">
    <h2 class="main-title">Suggested Items</h2>

    <div class="suggested-container">

        {{#each suggestedItems}}

        <div class="items-overview">
            <h3 class="newItemH3">{{this.title}}</h3>
            <div class="icons">
                <img class="suggested-icon" src='{{this.img_link}}'>
                <button class="new-item-button" id="{{this.id}}" onclick="window.location.href='/inventory/suggested-item/{{this.id}}'">
                    <img class="help-icon" src="../../public/imgs/help.svg" alt="">
                </button>
            </div>
        </div>

        {{/each}}


</section>